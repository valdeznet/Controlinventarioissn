@model Controlinventarioissn.Data.Entities.Sector

@{
    ViewData["Title"] = "Delete";
}
<flash dismissable="true" />
<h1>Borrar</h1>

<h3>¿Esta seguro de querer borrar?</h3>
<div>

    <partial name="_SectorDetails" />

    <form asp-action="DeleteSector">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-outline-danger" /> |
        <a asp-action="Details" asp-route-id="@Model?.Delegacion.Id" class="btn btn-outline-success">Regresar</a>
    </form>
</div>