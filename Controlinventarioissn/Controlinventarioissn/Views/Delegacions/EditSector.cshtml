@model Controlinventarioissn.Models.SectorViewModel

@{
    ViewData["Title"] = "Edit";
}
<flash dismissable="true" />
<h1>Editar</h1>

<h4>Delegación</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="EditSector">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="DelegacionId" />

            <partial name="_Sector" />

            <div class="form-group mt-2">
                <input type="submit" value="Guardar" class="btn btn-outline-primary" />
                <a asp-action="Details" asp-route-id="@Model.DelegacionId" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }
