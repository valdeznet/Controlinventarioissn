@model Controlinventarioissn.Models.AddDepositoEquipamientoViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<h4>Deposito / Equipamiento</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="AddDeposito" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EquipamientoId" />
            <input type="hidden" asp-for="Depositos" />
            <div class="form-group">
                <label asp-for="DepositoId" class="control-label"></label>
                <select asp-for="DepositoId" asp-items="Model.Depositos" class="form-control"></select>
                <span asp-validation-for="DepositoId" class="text-danger"></span>
            </div>
            <div class="form-group mt-2">
                <input type="submit" value="Crear" class="btn btn-outline-primary" />
                <a asp-action="Details" asp-route-id="@Model.EquipamientoId" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }
